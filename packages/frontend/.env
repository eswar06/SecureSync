# SecureSync Pro Frontend Environment Configuration
# Copy this file to .env and update the values

# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_SOCKET_URL=http://localhost:5000
VITE_WS_URL=ws://localhost:5000

# Application Configuration
VITE_APP_NAME=SecureSync Pro
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Authentication
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_AUTH_REDIRECT_URL=http://localhost:3000/auth/callback

# WebRTC Configuration
VITE_TURN_SERVER_URL=turn:localhost:3478
VITE_TURN_USERNAME=securesync
VITE_TURN_CREDENTIAL=securesync2024

# AI Services
VITE_OPENAI_API_KEY=your-openai-api-key
VITE_AI_FEATURES_ENABLED=true

# Feature Flags
VITE_RECORDING_PREVENTION_ENABLED=true
VITE_PRIVATE_VOICE_ENABLED=true
VITE_AI_TRANSCRIPTION_ENABLED=true
VITE_DOCUMENT_SECURITY_ENABLED=true

# Analytics and Monitoring
VITE_ANALYTICS_ENABLED=false
VITE_SENTRY_DSN=your-sentry-dsn
VITE_LOG_LEVEL=info

# Development Tools
VITE_DEV_TOOLS_ENABLED=true
VITE_MOCK_API_ENABLED=false

# PWA Configuration
VITE_PWA_ENABLED=true
VITE_PWA_NAME=SecureSync Pro
VITE_PWA_SHORT_NAME=SecureSync
VITE_PWA_DESCRIPTION=Enterprise collaboration platform with breakthrough security
